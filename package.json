{
  "name": "dmm-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/dynamic-amm/dmm-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "prepare-constants:dev": "mustache config/dev.json src/config/constants/index.template.ts > src/config/constants/index.ts",
    "prepare-subgraph:dev": "mustache config/dev.json subgraph.template.yaml > subgraph.yaml",
    "prepare:dev": "yarn prepare-constants:dev && yarn prepare-subgraph:dev",
    "prepare-constants:staging": "mustache config/staging.json src/config/constants/index.template.ts > src/config/constants/index.ts",
    "prepare-subgraph:staging": "mustache config/staging.json subgraph.template.yaml > subgraph.yaml",
    "prepare:staging": "yarn prepare-constants:staging && yarn prepare-subgraph:staging",
    "prepare-constants:production": "mustache config/production.json src/config/constants/index.template.ts > src/config/constants/index.ts",
    "prepare-subgraph:production": "mustache config/production.json subgraph.template.yaml > subgraph.yaml",
    "prepare:production": "yarn prepare-constants:production && yarn prepare-subgraph:production",
    "prepare-constants:mumbai": "mustache config/mumbai.json src/config/constants/index.template.ts > src/config/constants/index.ts",
    "prepare-subgraph:mumbai": "mustache config/mumbai.json subgraph.template.yaml > subgraph.yaml",
    "prepare:mumbai": "yarn prepare-constants:mumbai && yarn prepare-subgraph:mumbai",
    "prepare-constants:matic": "mustache config/matic.json src/config/constants/index.template.ts > src/config/constants/index.ts",
    "prepare-subgraph:matic": "mustache config/matic.json subgraph.template.yaml > subgraph.yaml",
    "prepare:matic": "yarn prepare-constants:matic && yarn prepare-subgraph:matic",
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "create-local": "graph create dynamic-amm/dynamic-amm --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy dynamic-amm/dynamic-amm --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "deploy": "graph deploy piavgh/dmm-exchange-ropsten --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-staging": "graph deploy edwardevans094/stagingdmm --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-dev": "graph deploy piavgh/dmm-exchange-ropsten --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-semi-prod": "graph deploy edwardevans094/dmm-prod --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-prod": "graph deploy dynamic-amm/dynamic-amm --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-mumbai": "graph deploy piavgh/dmm-exchange-mumbai --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-matic": "graph deploy dynamic-amm/dynamic-amm-matic --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "watch-local": "graph deploy dynamic-amm/dynamic-amm --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001",
    "prettier": "./node_modules/.bin/prettier --write src/mappings/*"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.2",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "mustache": "^4.2.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
